@model Summernote

<link rel="stylesheet" href="~/lib/summernote/summernote-bs5.min.css" asp-append-version="true" />
<script src="~/lib/summernote/summernote-bs5.min.js"></script>

<script>
    $(document).ready(function() {
        $('@Model.IDEditor').summernote({
            height: @Model.Height,
            toolbar: @Html.Raw(Model.Toolbar),
            container: 'body'
        });
    });
</script>